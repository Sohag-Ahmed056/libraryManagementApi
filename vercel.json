{
  "version": 2,
  "functions": {
    "src/api/index.ts": {
      "runtime": "@vercel/node"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/src/api/index.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/src/api/index.ts"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
